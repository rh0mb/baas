<div class="container">   
  <div class="row-fluid">
    <div class="span12">

      <div class="row-fluid">
        <h4>Aqui va la descripcion</h4>
      </div>
      
      <form id="ecot" action="#">
      <div class="row-fluid">
        <div class="span3">
          <label>Base</label>
          <input type="text" name="b" placeholder="en metros">
        </div>
        <div class="span3">
          <label>Altura</label>
          <input type="text" name="h" placeholder="en metros">
        </div>
        <div class="span3">
        </div>
        <div class="span3">
        </div>
      </div>


                <input type="text" name="d" placeholder="en metros" class="hideme" value=<%= banamex_parser %>>

      <div class="row-fluid">
        <div class="span3">
          <input type="button" class="btn-u" value="Calcular" onclick="cotizar();" />
        </div>
      </div><br>

      <div class="row-fluid">
        <div class="span3">
          <label>Area (m2)</label>
          <input type="text" name="a" readonly="readonly">
        </div>
      </div>

      <div class="row-fluid">
        <div class="span3">
          <label>Precio Total (MXN)</label>
          <input type="text" name="priceMxn" readonly="readonly">
        </div>
        <div class="span3">
          <label>Precio por metro (MXN)</label>
          <input type="text" name="priceMMxn" readonly="readonly">
        </div>
      </div>

      <div class="row-fluid">
        <div class="span3">
          <label>Precio Total (MXN) con IVA</label>
          <input type="text" name="priceMxnIva" readonly="readonly">
        </div>
        <div class="span3">
          <label>Precio por Metro (MXN) con IVA</label>
          <input type="text" name="priceMMxnIva" readonly="readonly">
        </div>
      </div>

      <div class="row-fluid">
        <div class="span3">
          <label>Precio Total (USD)</label>
          <input type="text" name="priceUsd" readonly="readonly">
        </div>
        <div class="span3">
          <label>Precio por Metro (USD)</label>
          <input type="text" name="priceMUsd" readonly="readonly"> <br>
        </div>
      </div>

      <div id="box"></div>
      <div id="box2"></div>

  </form>

     
    </div><!--/span12-->
  </div>
</div>



